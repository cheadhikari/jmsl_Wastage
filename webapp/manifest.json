{
	"_version": "1.7.0",
	"sap.app": {
		"id": "isr.jmsl_Wastage",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "servicecatalog.connectivityComponent",
			"version": "0.0.0"
		},
		"dataSources": {
			"ZWASTAGE_SRV": {
				"uri": "/sap/opu/odata/sap/ZWASTAGE_SRV/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/metadata.xml"
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": ["sap_hcb", "sap_belize"]
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "isr.jmsl_Wastage.view.App",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {},
				"sap.ushell": {},
				"sap.collaboration": {},
				"sap.ui.comp": {},
				"sap.uxap": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "isr.jmsl_Wastage.i18n.i18n"
				}
			},
			"oModel": {
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "OneWay",
					"defaultCountMode": "Request"
				},
				"dataSource": "ZWASTAGE_SRV",
				"preload": true
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewPath": "isr.jmsl_Wastage.view",
				"controlId": "app",
				"controlAggregation": "pages"
			},
			"targets": {
				"Selection": {
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true,
					"viewName": "Selection",
					"viewLevel": 1
				},
				"LineItems": {
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true,
					"viewName": "LineItems",
					"viewLevel": 2
				}
			},
			"routes": [{
				"name": "Selection",
				"pattern": "",
				"titleTarget": "",
				"greedy": false,
				"target": ["Selection"]
			}, {
				"name": "LineItems",
				"pattern": "Items/{Movetype},{Site},{Location}",
				"titleTarget": "",
				"greedy": false,
				"target": ["LineItems"]
			}]
		}
	}
}